      <bcf:dtarget>ALL</bcf:dtarget>
      <bcf:dmode order="1">original</bcf:dmode>
      <bcf:dmode order="2">romanised</bcf:dmode>
      <bcf:dmode order="3">uniform</bcf:dmode>
      <bcf:dmode order="4">translated</bcf:dmode>
    </bcf:displaymode>
    <bcf:displaymode>
      <bcf:dtarget>title</bcf:dtarget>
      <bcf:dtarget>booktitle</bcf:dtarget>
      <bcf:dmode order="1">romanised</bcf:dmode>
      <bcf:dmode order="2">uniform</bcf:dmode>
      <bcf:dmode order="3">translated</bcf:dmode>
      <bcf:dmode order="4">original</bcf:dmode>
    </bcf:displaymode>
  </bcf:displaymodes>
  <!-- DATA-SOURCE MAPPING -->
  <bcf:sourcemap>
    <bcf:maps datatype="bibtex" map_overwrite="1">
      <bcf:map>
        <bcf:per_datasource>bibtex-aliases.bib</bcf:per_datasource>
        <!-- Doesn't exist, just testing multi for this option-->
        <bcf:per_datasource>bibtex-aliases2.bib</bcf:per_datasource>
        <bcf:map_step map_type_source="MISC" map_final="1"/>
        <bcf:map_step map_field_set="KEYWORDS" map_field_value="keyw1, keyw2"/>
      </bcf:map>
      <bcf:map>
        <bcf:map_step map_type_source="CONVERSATION" map_type_target="CUSTOMA" map_final="1"/>
        <bcf:map_step map_field_set="VERBA" map_origentrytype="1"/>
        <bcf:map_step map_field_set="VERBB" map_field_value="somevalue"/>
        <bcf:map_step map_field_set="VERBC" map_field_value="somevalue"/>
      </bcf:map>
      <bcf:map>
        <bcf:map_step map_type_source="CHAT" map_type_target="CUSTOMB"/>
      </bcf:map>
      <bcf:map>
        <bcf:per_datasource>bibtex-aliases.bib</bcf:per_datasource>
        <bcf:map_step map_field_set="ABSTRACT" map_null="1"/>
        <bcf:map_step map_field_source="CONDUCTOR" map_field_target="NAMEA"/>
        <bcf:map_step map_field_source="GPS" map_field_target="USERA"/>
        <bcf:map_step map_field_source="PARTICIPANT" map_field_target="NAMEA"/>
        <bcf:map_step map_field_source="USERB" map_field_target="USERC"/>
        <bcf:map_step map_field_set="USERC" map_origfieldval="1"/>
      </bcf:map>
      <bcf:map>
        <bcf:map_step map_field_source="PUBMEDID" map_field_target="EPRINT"
                      map_final = "1"/>
        <bcf:map_step map_field_set="EPRINTTYPE" map_origfield="1"/>
        <bcf:map_step map_field_set="USERD" map_field_value="Some string of things"/>
      </bcf:map>
      <bcf:map>
        <bcf:per_datasource>bibtex-aliases.bib</bcf:per_datasource>
        <bcf:per_type>MISC</bcf:per_type>
        <bcf:map_step map_field_set="USERB" map_null="1"/>
        <bcf:map_step map_field_set="USERE" map_field_value="a string"/>
        <bcf:map_step map_field_set="USERF" map_null="1"/>
      </bcf:map>
      <bcf:map>
        <bcf:per_type>REPORT</bcf:per_type>
        <bcf:map_step map_field_source="LISTA" map_match="BADMATCH" map_final="1"/>
        <bcf:map_step map_field_set="LISTA" map_null="1"/>
      </bcf:map>
      <bcf:map>
        <bcf:map_step map_field_source="LISTB" map_match="\A(\S{2})" map_replace="REP$1CED"/>
      </bcf:map>
      <bcf:map>
        <bcf:per_type>UNPUBLISHED</bcf:per_type>
        <bcf:map_step map_field_source="LISTC" map_field_target="INSTITUTION" map_match="\A(\S{2})" map_replace="REP$1CED"/>
      </bcf:map>
    </bcf:maps>
  </bcf:sourcemap>
  <bcf:labelalphatemplate type="global">
    <bcf:labelelement order="1">
      <bcf:labelpart final="1">shorthand</bcf:labelpart>
      <bcf:labelpart>label</bcf:labelpart>
      <bcf:labelpart substring_width="3" substring_side="left" ifnamecount="1">labelname</bcf:labelpart>
      <bcf:labelpart substring_width="1" substring_side="left" namecount="2">labelname</bcf:labelpart>
    </bcf:labelelement>
    <bcf:labelelement order="2">
      <bcf:labelpart substring_width="2" substring_side="right">year</bcf:labelpart>
    </bcf:labelelement>
  </bcf:labelalphatemplate>
  <bcf:labelalphatemplate type="misc">
    <bcf:labelelement order="1">
      <bcf:labelpart>label</bcf:labelpart>
      <bcf:labelpart substring_width="vf" substring_fixed_threshold="2" substring_compound="1">labelname</bcf:labelpart>
    </bcf:labelelement>
    <bcf:labelelement order="2">
      <bcf:labelpart>-</bcf:labelpart>
    </bcf:labelelement>
    <bcf:labelelement order="3">
      <bcf:labelpart substring_width="2" substring_side="right">title</bcf:labelpart>
    </bcf:labelelement>
  </bcf:labelalphatemplate>
  <bcf:labelalphatemplate type="book">
    <bcf:labelelement order="1">
      <bcf:labelpart>label</bcf:labelpart>
      <bcf:labelpart substring_width="v" substring_width_max="3">labelname</bcf:labelpart>
    </bcf:labelelement>
  </bcf:labelalphatemplate>
  <bcf:labelalphatemplate type="report">
    <bcf:labelelement order="1">
      <bcf:labelpart substring_width="l">labelname</bcf:labelpart>
    </bcf:labelelement>
  </bcf:labelalphatemplate>
