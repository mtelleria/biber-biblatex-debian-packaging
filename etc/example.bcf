  <bcf:nosorts>
    <!-- strip prefixes like 'al-' when sorting name fields -->
    <nosort field="type_names" value="\A\p{L}{2}\p{Pd}"/>
    <!-- strip diacritics when sorting name fields -->
    <nosort field="type_names" value="[\x{2bf}\x{2018}]"/>
